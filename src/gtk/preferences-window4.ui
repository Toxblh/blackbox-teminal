<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="TerminalPreferencesWindow2" parent="AdwApplicationWindow">
    <property name="can-focus">False</property>
    <child>
      <object class="AdwLeaflet">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <child>
          <object class="GtkBox" id="side_box">
            <property name="orientation">vertical</property>
            <child>
              <object class="AdwHeaderBar" id="side_headerbar">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="show-start-title-buttons">True</property>
                <property name="show-end-title-buttons">False</property>
                <child type="title">
                  <object class="GtkLabel">
                    <property name="visible">0</property>
                    <property name="label">Preferences</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkSearchBar" id="search_bar">
                <property name="search-mode-enabled">1</property>
                <style>
                  <class name="inline"/>
                </style>
                <property name="child">
                  <object class="GtkEntry">
                    <property name="visible">0</property>
                    <property name="placeholder-text">Search</property>
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkEntry">
                <property name="visible">0</property>
                <property name="placeholder-text">Search</property>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow">
                <property name="focusable">1</property>
                <property name="hscrollbar-policy">never</property>
                <property name="vexpand">1</property>
                <property name="child">
                  <object class="GtkViewport">
                    <property name="child">
                      <object class="GtkListBox" id="side_list_box"/>
                    </property>
                  </object>
                </property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkSeparator">
            <property name="orientation">vertical</property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="main_box">
            <property name="hexpand">1</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="AdwHeaderBar" id="main_headerbar">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="show-start-title-buttons">False</property>
                <property name="show-end-title-buttons">True</property>
              </object>
            </child>
            <child>
              <object class="GtkEntry">
                <property name="visible">0</property>
                <property name="placeholder-text">Search</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
